// 15 DAYS TRIAL BUTTOM
.pricing-plan
   .container
      .pricing-plan-grids
          .pricing-plan-left
             h2 !{footer.try_it}
             span #{home.permanence}
          .pricing-plan-right
             a.price-btn.modal-trigger(href='#pruebaGratis') #{header.free_trial}
          .clearfix
// COSTUMERS SERVICES CONTACT AREA
.servCliente
   .container
      .col.s12
         ul
            li
               a #{helpcenter.customer_service.text2}
            li.tel.hover-anim
               a(href='#{helpcenter.customer_service.phone.link}', title='#{helpcenter.customer_service.phone.alt_tag}')
                  i.icon-phone
                  | !{helpcenter.customer_service.phone.text_tel}
            li.mail.hover-anim
               a(href='#{helpcenter.customer_service.email.link}', title='#{helpcenter.customer_service.email.alt_tag}')
                  i.icon-envelope
                  | #{helpcenter.customer_service.email.title}
// FOOTER
footer.page-footer
                    .container.padTop50.padBot50
                        .row.hover-anim
                            .col.l3.s12.footCol
                                h6 #{external.titles.help}
                                ul.footLinks
                                    li
                                        a(href='#{external.helpcenter.url}', title='#{external.helpcenter.anchor}') #{external.helpcenter.anchor}
                                    li
                                        a(href='#{external.tutorials.url}', title='#{external.tutorials.anchor}') #{external.tutorials.anchor}
                                    li
                                        a(href='http://www.upplication.com/blog/', title='#{external.helpcenter.anchor}') #{external.blog.anchor}
                            .col.l3.s12.footCol
                                h6 #{external.titles.uppl}
                                ul.uppOnFoot.footLinks(type='circle')
                                    li
                                        a(href='#tabs', title='#{external.features.anchor}') #{external.features.anchor}
                                    li
                                        a(href='#about', title='#{external.create_apps.anchor}') #{external.create_apps.anchor}
                                    li
                                        a(href='#tabs', title='#{external.apps.anchor}') #{external.apps.anchor}
                                    li
                                        a(href='#pricing', title='#{external.pricing.anchor}') #{external.pricing.anchor}
                            .col.l3.s12.footCol
