        script.
            $(function () {
                $(".element").typed({
                    strings: !{JSON.stringify(home.slider.text)},
                    typeSpeed: 100,
                    startDelay: 200,
                    backDelay: 2500,
                    loop: true
                });
            });
        // smooth scrolling
        script(type='text/javascript').
            $(document).ready(function () {
                $("#clientsCarousel").owlCarousel({
                    autoPlay: 3000, // Set AutoPlay to 3 seconds
                    items: 4,
                    itemsDesktop: [1199, 3],
                    itemsDesktopSmall: [979, 3]
                });
                $('.scrollspy').scrollSpy();
                $('ul.tabs').tabs();
                $('.modal-trigger').leanModal({
                    opacity: .7
                });
                $('.parallax').parallax();
                $(".button-collapse").sideNav();
                $('.slider').slider({
                    indicators: false,
                    interval: 4000
                });
                var options = [
                    {selector: '#tabs', offset: 300, callback: 'Materialize.showStaggeredList("#tabs")'},
                    {selector: '#about', offset: 300, callback: 'Materialize.showStaggeredList(".timeline")'},
                    {selector: '#pricing', offset: 300, callback: 'Materialize.showStaggeredList(".incluye")'},
                ];
                Materialize.scrollFire(options);
                // Animate.css for web elements:
                var $imgMove = $('.imgMove'),
                        $imgFlash = $('.imgFlash');
                $imgMove.mouseenter(function () {
                    $(this).addClass("animated");
                    $(this).addClass("pulse");
                });
                $imgMove.mouseleave(function () {
                    $(this).removeClass("animated");
                    $(this).removeClass("pulse");
                });
                $imgFlash.mouseenter(function () {
                    $(this).addClass("animated");
                    $(this).addClass("rubberBand");
                });
                $imgFlash.mouseleave(function () {
                    $(this).removeClass("animated");
                    $(this).removeClass("rubberBand");
                });
                $('.social-share').click(function () {
                    $(".social-nav").toggleClass("social-show");
                });
                $('#cookies-accept').click(function () {
                    $("#habla_window_div").addClass("bottomFix");
                });
                $('.lang-link').dropdown({
                      inDuration: 300,
                      outDuration: 225,
                      constrain_width: false,
                      gutter: 0,
                      belowOrigin: false,
                      alignment: 'left'
                    }
                  );
            });
        // START MOBILE TOP NAVIGATION MENU
        script.
            $(function () {
                var $pull = $('#pull');
                menu = $('nav ul');
                menuHeight = menu.height();
                $pull.on('click', function (e) {
                    e.preventDefault();
                    menu.slideToggle();
                });
                $(window).resize(function () {
                    var w = $(window).width();
                    if (w > 320 && menu.is(':hidden')) {
                        menu.removeAttr('style');
                    }
                });
                $pull.click(function () {
                    $(".hide-ul").removeClass("hide-tabs");
                });
                var windowWidth = $(window).width();
                if(windowWidth < 769){
                    $('.hide-tab').click(function () {
                        $(".hide-ul").addClass("hide-tabs");
                    });
                }
            });

        include base/scripts-thirdparties

