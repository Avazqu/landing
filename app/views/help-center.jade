    body
        // MAIN HEADER
        #home.header
            #move-top.top-header
                .container
                    .logo
                        a(href='index.html')
                            img(src='#{localConfig.base_path}/images/home/icons/upplication.png', title='Upplication logo')
                    nav.top-nav
                        ul.top-nav.hide-ul
                            li
                                each language in langs.codes
                                    if lang == language.language_country
                                        a.lang-link(href="#", data-activates='dropdown-lang')
                                            i.fa.fa-globe
                                            | !{language.language_short_code}
                                ul#dropdown-lang.dropdown-content
                                    each lang in langs.codes
                                        a.hide-tab.lang-link(href="#{routing[template][lang.language_country]}", data-lang="#{lang.language_country}") #{lang.language_name}
                            li
                                a.hide-tab(href='#about', title='') #{header.nav.how}
                            li
                                a.hide-tab(href='#tabs', title='') #{header.nav.apps}
                            li
                                a.hide-tab(href='#pricing', title='') #{header.nav.prices}
                            li
                                a.hide-tab(href='http://www.upplication.com/blog/', target='_blank', title='') #{external.blog.anchor}
                            li.only-show-small-devises.phone-kerning
                                a #{header.telephone}
                            li.only-show-small-devises
                                a.modal-trigger(href='#pruebaGratis', title='') #{header.free_trial}
                            li.enter-bold
                                a.hide-tab(href="#{$.external.dashboard.url}")
                                    i.fa.fa-lock
                                    | #{header.enter}
                        a#pull(href='')
                            img(src='#{localConfig.base_path}/images/home/icons/nav-icon.png', title='menu')
                    .contact-info
                        .contact-info-grids
                            .contact-info-left
                                a.chat.modal-trigger(href='#pruebaGratis') #{header.free_trial}
                            .contact-info-right
                                a.modal-trigger(href='#allContact') #{header.telephone}
                                a.support(href='#{helpcenter.customer_service.email.link}', title='#{helpcenter.customer_service.email.alt_tag}') #{helpcenter.customer_service.email.title}
                            .clearfix
                    .clearfix
        // MODAL PRUEBA GRATIS
        #pruebaGratis.modal
            include ./includes/modal-free.jade
        // MODAL CONTACT
        #allContact.modal
            include ./includes/modal-contact.jade
        // HELP CENTER
        #help-center.container
              h4.animated.fadeIn #{helpcenter._name}
              .row
                .col.s12.m6.help-center-grid
                  ul.collection
                    li.collection-item.avatar.animated.fadeInRight
                      i.material-icons.circle.blue perm_identity
                      span.title #{helpcenter.section.upp}
                      a(href='#{helpcenter.uservoice.link}', title='#{helpcenter.uservoice.title}', target='_blank')
                          i.fa.fa-circle
                          | #{helpcenter.uservoice.title}
                      a(href='#{helpcenter.knowledgebase.link}', title='#{helpcenter.knowledgebase.title}', target='_blank')
                          i.fa.fa-circle
                          | #{helpcenter.knowledgebase.title}
                    li.collection-item.avatar.animated.fadeInLeft
                      i.material-icons.circle.red play_arrow
                      span.title #{helpcenter.section.video}
                          a(href='#{helpcenter.video_1.link}', title='#{helpcenter.video_1.title}', target='_blank')
                              i.fa.fa-circle
                              | #{helpcenter.video_1.title}
                          a(href='#{helpcenter.video_2.link}', title='#{helpcenter.video_2.title}', target='_blank')
                              i.fa.fa-circle
                              | #{helpcenter.video_2.title}
                    li.collection-item.avatar.animated.fadeInRight
                      i.material-icons.circle.green description
                      span.title #{helpcenter.section.ebooks}
                          a(href='#{helpcenter.sucess.link}', title='#{helpcenter.sucess.title}', target='_blank')
                              i.fa.fa-circle
                              | #{helpcenter.sucess.title}
                          a(href='#{helpcenter.advantages.link}', title='#{helpcenter.advantages.title}', target='_blank')
                              i.fa.fa-circle
                              | #{helpcenter.advantages.title}
                    li.collection-item.avatar.animated.fadeInLeft
                      i.material-icons.circle.purple settings
                      span.title #{helpcenter.section.help}
                          a(href='#{helpcenter.wedoit.link}', title='#{helpcenter.wedoit.title}', target='_blank')
                              i.fa.fa-circle
                              | #{helpcenter.wedoit.title}
                          a(href='#{helpcenter.workshop.link}', title='#{helpcenter.workshop.title}', target='_blank')
                              i.fa.fa-circle
                              | #{helpcenter.workshop.title}
              .clearfix
        // CLIENTS, TRIAL, CONTACT and FOOTER
        include ./includes/footer.jade
        #cookies
            i#cookies-accept.fa.fa-times.close
            p !{footer.cookies}
        // vendor js
        script(type="text/javascript", src='#{localConfig.base_path}/bower_components/jquery/dist/jquery.min.js')
        script(type="text/javascript", src='#{localConfig.base_path}/bower_components/materialize/dist/js/materialize.min.js')
        script(type="text/javascript", src='#{localConfig.base_path}/bower_components/typed.js/dist/typed.min.js')
        script(type="text/javascript", src='#{localConfig.base_path}/bower_components/owlcarousel/owl-carousel/owl.carousel.min.js')
        // endvendor

        // build:js /scripts/app.v2.min.js
        script(type="text/javascript", src='/scripts/ua.js')
        script(type="text/javascript", src='/scripts/topic.js')
        script(type="text/javascript", src='/scripts/scroll.js')
        script(type="text/javascript", src='/scripts/v2/cookies.js')
        script(type="text/javascript", src='/scripts/v2/register.js')
        script(type="text/javascript", src='/scripts/v2/lang.js')
