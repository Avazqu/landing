                                        a.lang-link(href="#", data-activates='dropdown-lang')
                                            i.fa.fa-globe
                                            | !{language.language_short_code}
                                ul#dropdown-lang.dropdown-content
                                    each lang in langs.codes
                                        a.hide-tab.lang-link(href="#{routing[template][lang.language_country]}", data-lang="#{lang.language_country}") #{lang.language_name}
