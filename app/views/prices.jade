extends base/v2/base

block vars
   - var title = prices._meta_title
   - var description = prices._meta_description
   - var keywords = prices._keywords
   - var cssClass = 'prices'

block css

   // build:less(app) /styles/less/test.less
   link(rel='stylesheet', href='/styles/less/prices/prices.less')
   // endbuild

block content

    //- MIXIN Declarations
    mixin webapp
        a(href='#', title='#{prices.rates.app_web}')
            i.fa.fa-desktop
            i.fa.fa-android
            i.fa.fa-apple
            | #{prices.rates.app_web}


    //- CLIENTS
    .clients
        .container
            //- PRICING PLANS
            #pricing.pricing-tables.pricing-tables-mobile.scrollspy
                .plans-head.text-center
                    h4.headTitles.noMargBot #{prices.rates.rate_intro}
                    p #{prices.rates.rate_sub}
                    .col-xs-12.col-sm-offset-3.col-sm-8.prices-common-features
                        ul
                            li
                                i.fa.fa-commenting-o
                                | #{prices.rates.sub1}
                            li
                                i.fa.fa-shopping-cart
                                | !{prices.rates.sub2}
                            li
                                i.fa.fa-tablet
                                | #{prices.rates.sub3}
                            li
                                i.fa.fa-graduation-cap
                                | #{prices.rates.sub4}
                    .clearfix
                .row
                    .col.s12.offset-m2.m8
                        ul.tabs
                            li.tab.col.s12.m6
                                a.active(href='#mensual', title='#{prices.rates.monthly}') !{prices.rates.monthly}
                            li.tab.col.s12.m6
                                a(href='#anual', title='#{prices.rates.yearly}') !{prices.rates.yearly}
                    // Plan Mensual
                    #mensual.col.s12
                        .col-xs-12.col-md-3.mobilePrices
                            .pricing-table-grid
                                h3 #{prices.rates.basic}
                                ul
                                    li
                                        span
                                            | 9,95
                                            em #{prices.rates.euro_month}
                                    li
                                        //- MIXIN Use
                                        +webapp
                                    li
                                        a(href='#', title='#{prices.rates.icons.statistics}')
                                            i.fa.fa-bar-chart
                                            | !{prices.rates.statistics}
                                    li
                                        a(href='#', title='#{prices.rates.icons.bullhorn}')
                                            i.fa.fa-bullhorn
                                            | !{prices.rates.bullhorn}
                                    li
                                        a(href='#', title='#{prices.rates.icons.sign}')
                                            i.fa.fa-pencil
                                            | !{prices.rates.sign}
                                    li
                                        a(href='#', title='#{prices.rates.icons.no_push}')
                                            i.fa.fa-bell-slash
                                            | !{prices.rates.no_push}
                                a.order-btn.modal-trigger(href='#free-trial', title='#{register.call_to_action}') #{register.call_to_action}
                        .col-xs-12.col-md-3.mobilePrices
                            img.responsive-img.best-seller(src='#{localConfig.base_path}/images/home/icons/badgeWinner.png', title='#{prices.rates.topsale}', alt='#{prices.rates.topsale}')
                            .pricing-table-grid.with-hover
                                h3 #{prices.rates.proff}
                                ul
                                    li
                                        span
                                            | 29,95
