                                a.order-btn.modal-trigger(href='#pruebaGratis') #{home.register.call_to_action}
                        .clearfix
                        .col-xs-12
                            .alert.alert-info(role='alert')
                                span.fa-stack.fa-lg
                                    i.fa.fa-money.fa-stack-1x
                                    i.fa.fa-ban.fa-stack-2x
                                span.monthless !{prices.rates.monthless}
                .col-xs-12.tax.center
                    p
                        i.fa.fa-asterisk
                        | !{prices.rates.with_tax}
